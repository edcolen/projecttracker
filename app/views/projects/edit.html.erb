<p><%= link_to "back to dashboard", dashboard_path %></p>

<h2>Update Project</h2>

<%= form_with(model: @project, local: true) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :start_date %>
  <%= f.date_select :start_date %>

  <%= f.label :end_date %>
  <%= f.date_select :end_date %>

  <%= f.submit "Update project" %>
<% end %>
